node {
        stage('Checkout') 
            
                git branch: 'master', url: 'https://github.com/anuragsinghai2010/JenkinsFile.git'
                
                echo "Code is cloned from git"
                
                mkdir abc
    
        stage('Run JMeter Test') 
     
            sh '/Users/anuragjain/Downloads/apache-jmeter-5.4.1/bin/jmeter.sh -n -t /Users/anuragjain/.jenkins/workspace/Jmeter_test/Calculate_location.jmx -l result.jtl'
            
}
